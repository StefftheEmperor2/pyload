{
  "manifest_version": 2,
  "name": "pyLoad",
  "version": "1.0",

  "description": "Handles interactive Captcha",

  "icons": {
    "48": "icons/pyload.svg",
    "96": "icons/pyload.svg"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://filecrypt.cc/*",
    "*://www.filecrypt.cc/*",
    "*://filecrypt.co/*",
    "*://www.filecrypt.co/*",
    "*://cutcaptcha.com/*",
    "*://gstatic.com/*",
    "*://google.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://filecrypt.cc/*",
        "*://filecrypt.co/*",
        "*://www.filecrypt.cc/*",
        "*://www.filecrypt.co/*"
      ],
      "js": [
        "jsrsasign-all-min.js",
        "captcha.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "page-scripts/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}